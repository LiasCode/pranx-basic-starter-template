    "preact": "^10.26.9",
    "preact-iso": "^2.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.7",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
